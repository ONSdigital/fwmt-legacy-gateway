      <xs:element name="SubProperties" type="AdditionalPropertyDefinitionCollectionType" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            Sub-properties of the Additional Property Definition.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Active" type="xs:boolean" minOccurs="1" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            Indicates if the Additional Property Definition is active or not.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>

  <xs:element name="AdditionalPropertyDefinitionCollection" type="AdditionalPropertyDefinitionCollectionType" />
  <xs:complexType name="AdditionalPropertyDefinitionCollectionType">
    <xs:annotation>
      <xs:documentation>
        Defines a collection of Additional Properties Definitions.
      </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="AdditionalPropertyDefinition" type="AdditionalPropertyDefinitionType" minOccurs="0"
                  maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation>
            Collection of Additional Property Definitions.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>


  <xs:element name="ReasonCodeItem" type="ReasonCodeItemType" />
  <xs:complexType name="ReasonCodeItemType">
    <xs:annotation>
      <xs:documentation>
        Defines the elements of a ReasonCode.
      </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="Guid" type="GuidType" minOccurs="1" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            Unique identifier for a ReasonCode.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Code" minOccurs="1" maxOccurs="1" type="ShortReferenceType">
        <xs:annotation>
          <xs:documentation>
            Code for the ReasonCode.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Description" minOccurs="0" maxOccurs="1" type="LongReferenceType">
        <xs:annotation>
          <xs:documentation>
            Description of the ReasonCode.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Type" minOccurs="0" maxOccurs="1" type="LongReferenceType">
        <xs:annotation>
          <xs:documentation>
            Type of the ReasonCode.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>

  <xs:element name="ReasonCodeItemCollection" type="ReasonCodeItemCollectionType" />
  <xs:complexType name="ReasonCodeItemCollectionType">
    <xs:annotation>
      <xs:documentation>
        Defines a collection of ReasonCodes.
      </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="ReasonCodeItem" type="ReasonCodeItemType" minOccurs="0" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation>
            Collection of ReasonCodes.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>


  <xs:element name="Bulletin" type="BulletinType" />
  <xs:complexType name="BulletinType">
    <xs:annotation>
      <xs:documentation>
        Defines the elements of a bulletin.
      </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="Reference" type="LongReferenceType" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            Unique reference for a bulletin. If not provided a reference will be generated for you. When a bulletin is created, if a bulletin with the same reference exists for the users specified it will be updated or created if the users specified do not already have this bulletin.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Category" minOccurs="0" maxOccurs="1" type="LongReferenceType">
        <xs:annotation>
          <xs:documentation>
            Category for the bulletin.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Subject" minOccurs="1" maxOccurs="1" type="LongTextType">
        <xs:annotation>
          <xs:documentation>
            Subject of the bulletin.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Action" minOccurs="0" maxOccurs="1" type="LongTextType">
        <xs:annotation>
          <xs:documentation>
            Action of the bulletin, can be any URL. For example a form can be launched by id or form name using the following URL structure: totalmobile://forms/capture/{id} or totalmobile://forms/capture/?formname={test%20form%20name}.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ActionText" type="LongReferenceType" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            Text to describe the action.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Body" minOccurs="1" maxOccurs="1" type="xs:string">
        <xs:annotation>
          <xs:documentation>
            Body of the bulletin in plain text or Markdown format. Html will be rejected.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Payload" minOccurs="0" maxOccurs="1" type="BulletinPayloadItemCollectionType">
        <xs:annotation>
          <xs:documentation>
            Payload of the bulletin. When an action is provided this payload is made available to the action, for example if the action is to fill in a form the payload will be mapped into the form.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Recipients" minOccurs="1" maxOccurs="1" type="UserTypeCollectionType">
        <xs:annotation>
          <xs:documentation>
            Recipients of the bulletin.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>

  <xs:element name="BulletinPayloadItemCollection" type="BulletinPayloadItemCollectionType" />
  <xs:complexType name="BulletinPayloadItemCollectionType">
    <xs:annotation>
      <xs:documentation>
        Defines a collection of bulletin payload items.
      </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="BulletinPayloadItem" type="BulletinPayloadItemType" minOccurs="0" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation>
            Collection of bulletin payload items.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>


  <xs:element name="BulletinPayloadItem" type="BulletinPayloadItemType" />
  <xs:complexType name="BulletinPayloadItemType">
    <xs:annotation>
      <xs:documentation>
        Defines a bulletin payload item. A payload item can have either a value or a collection of child items.
      </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="Name" type="xs:string" minOccurs="1" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            Name of the item.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Value" type="xs:string" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            Value of the item, not required when items are provided.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Items" minOccurs="0" maxOccurs="1" type="BulletinPayloadItemCollectionType">
        <xs:annotation>
          <xs:documentation>
            Collection of payload items belonging to this item.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>

  <xs:element name="ContactUpdate" type="ContactUpdateType" />
  <xs:complexType name="ContactUpdateType">
    <xs:annotation>
      <xs:documentation>
        Defines optional updated details for a Contact.
      </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="Reference" type="LongReferenceType" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            Updated external unique identifier for the Contact.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ParentReference" type="LongReferenceType" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            Updated external unique identifier for the parent Contact for this Contact.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Name" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            Updated name of the Contact.
          </xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="100" />
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="Title" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            Updated title for the Contact.
          </xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="100" />
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="Address" type="AddressUpdateType" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            Updated Address details of the Contact.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Phone" type="PhoneNumberType" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            Updated phone number of the Contact.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="WorkPhone" type="PhoneNumberType" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            Updated Work phone number of the Contact.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="MobilePhone" type="PhoneNumberType" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            Updated mobile phone number of the Contact.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Email" type="ShortTextType" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            Updated email address of the Contact.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="URL" type="xs:anyURI" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            Updated URL of the Contact.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
    <xs:element name="ExtendedProperties" type="ContactExtensionType" nillable="true" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            Extended information about the Contact.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>

  <xs:element name="AddressUpdate" type="AddressUpdateType" />
  <xs:complexType name="AddressUpdateType">
    <xs:annotation>
      <xs:documentation>
        Defines optional updated details of an Address.
      </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="Name" maxOccurs="1" minOccurs="0" type="ShortTextType">
        <xs:annotation>
          <xs:documentation>
            Updated name of the Address.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Lines">
        <xs:annotation>
          <xs:documentation>
            Updated list of Address lines.
          </xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:sequence>
            <xs:element name="AddressLine" maxOccurs="5" minOccurs="0">
              <xs:annotation>
                <xs:documentation>
                  A single line of the Address.
                </xs:documentation>
              </xs:annotation>
              <xs:simpleType>
                <xs:restriction base="xs:string">
                  <xs:maxLength value="50" />
                </xs:restriction>
              </xs:simpleType>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="PostCode" maxOccurs="1" minOccurs="0">
        <xs:annotation>
          <xs:documentation>
            Updated postcode for the Address.
          </xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="10" />
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="GeoX" type="xs:float" nillable="true" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            Updated X mapping co-ordinate for the Address. (Equivalent of latitude)
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="GeoY" type="xs:float" nillable="true" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            Updated Y mapping co-ordinate for the Address. (Equivalent of longitude)
          </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>


</xs:schema>