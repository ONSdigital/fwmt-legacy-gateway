<?xml version="1.0" encoding="utf-8"?>

<xs:schema id="CommonTypes"
           targetNamespace="http://schemas.consiliumtechnologies.com/mobile/2009/03/CommonTypes.xsd"
           xmlns="http://schemas.consiliumtechnologies.com/mobile/2009/03/CommonTypes.xsd"
           xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">

  <xs:element name="Criteria" type="CriteriaType" />
  <xs:complexType name="CriteriaType">
    <xs:annotation>
      <xs:documentation>
        Defines search criteria.
      </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="Property" maxOccurs="1" minOccurs="1" type="xs:string">
        <xs:annotation>
          <xs:documentation>
            Property that the criteria relates to.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Value" type="xs:string" maxOccurs="1" minOccurs="1">
        <xs:annotation>
          <xs:appinfo source="maml">
            <table>
              <tableHeader>
                <row>
                  <entry>Wildcard</entry>
                  <entry>Example</entry>
                  <entry>Explaination</entry>
                </row>
              </tableHeader>
              <row>
                <entry>"*","%"</entry>
                <entry>John*</entry>
                <entry>Finds any string beginning with the letters “John”.</entry>
              </row>
              <row>
                <entry></entry>
                <entry>*Smith</entry>
                <entry>Finds any string ending with the letters “Smith”.</entry>
              </row>
              <row>
                <entry></entry>
                <entry>*Sm*</entry>
                <entry>Finds any string containing the letters “Sm”.</entry>
              </row>
              <row>
                <entry>“?”</entry>
                <entry>?BC</entry>
                <entry>Finds any string three characters long that has “B” and “C” as the second and third characters respectively.</entry>
              </row>
              <row>
                <entry>X:Y</entry>
                <entry>10:56</entry>
                <entry>The “range” wildcard finds any value between 10 and 56 inclusive. This wildcard can also be used with dates and to a lesser extent strings.</entry>
              </row>
              <row>
                <entry>=</entry>
                <entry>10</entry>
                <entry>Finds any value equal to 10.</entry>
              </row>
              <row>
                <entry>=</entry>
                <entry>=</entry>
                <entry>Finds NULL values i.e. blank fields.</entry>
              </row>
              <row>
                <entry>!=</entry>
                <entry>!=</entry>
                <entry>Finds any value that is not NULL i.e. non-blank fields.</entry>
              </row>
              <row>
                <entry>></entry>
                <entry>>10</entry>
                <entry>Finds any value greater than 10.</entry>
              </row>
              <row>
                <entry>&lt;</entry>
                <entry>&lt;10</entry>
                <entry>Finds any value less than 10.</entry>
              </row>
              <row>
                <entry>>=</entry>
                <entry>>=10</entry>
                <entry>Finds that is greater than or equal to 10.</entry>
              </row>
              <row>
                <entry>&lt;=</entry>
                <entry>&lt;=10</entry>
                <entry>Finds any value that is less than, or equal to, 10.</entry>
              </row>
              <row>
                <entry>&lt;></entry>
                <entry>&lt;>10 or !=10</entry>
                <entry>Finds any value not equal to 10.</entry>
              </row>
              <row>
                <entry>|</entry>
                <entry>1|2|3</entry>
                <entry>Finds values 1, 2 or 6.</entry>
              </row>
            </table>
          </xs:appinfo>
          <xs:documentation>
            Value to search for. This value can include wild cards - maybe include detail on what is accepted here e.g. range etc
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ParseAs" type="ParseAsType" minOccurs="1" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            Type of the Property.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>

  <xs:element name="ParseAs" type="ParseAsType" />
  <xs:simpleType name="ParseAsType">
    <xs:annotation>
      <xs:documentation>
        Defines the property types for search Criteria.
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="String">
        <xs:annotation>
          <xs:documentation>
            String.
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Numeric">
        <xs:annotation>
          <xs:documentation>
            Numeric
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Date">
        <xs:annotation>
          <xs:documentation>
            Date
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="DateTime">
        <xs:annotation>
          <xs:documentation>
            DateTime
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>

  <xs:element name="Document" type="DocumentType" />
  <xs:complexType name="DocumentType">
    <xs:annotation>
      <xs:documentation>
        Holds the location of a document its thumbnail
      </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="Location" type="xs:anyURI" minOccurs="1" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            Location of the document.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ThumbnailLocation" type="xs:anyURI" minOccurs="1" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            Location of the thumbnail for the document.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>

  <xs:element name="Address" type="AddressType" />
  <xs:complexType name="AddressType">
    <xs:annotation>
      <xs:documentation>
        Defines an Address.
      </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="Name" maxOccurs="1" minOccurs="0" type="ShortTextType">
        <xs:annotation>
          <xs:documentation>
            Name of the Address.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Lines">
        <xs:annotation>
          <xs:documentation>
            List of address lines that make up the address.
          </xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:sequence>
            <xs:element name="AddressLine" maxOccurs="5" minOccurs="0">
              <xs:annotation>
                <xs:documentation>
                  Represents a line of the address.
                </xs:documentation>
              </xs:annotation>
              <xs:simpleType>
                <xs:restriction base="xs:string">
                  <xs:maxLength value="50" />
                </xs:restriction>
              </xs:simpleType>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="HouseNo" type="ShortReferenceType" maxOccurs="1" minOccurs="0">
        <xs:annotation>
          <xs:documentation>
            House Number for the address. (Reserved for future use)
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="PostCode" maxOccurs="1" minOccurs="0">
        <xs:annotation>
          <xs:documentation>
            PostCode for the address.
          </xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="10" />
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="GeoX" type="xs:float" nillable="true" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            X mapping co-ordinate for the Address. (Equivalent of latitude)
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="GeoY" type="xs:float" nillable="true" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            Y mapping co-ordinate for the Address. (Equivalent of longitude)
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="URL" type="xs:anyURI" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            URL associated with the Address. (Reserved for future use. If the address is part of a ContactType, use ContactType.URL property instead)
          </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>

  <xs:element name="User" type="UserType" />
  <xs:complexType name="UserType">
    <xs:annotation>
      <xs:documentation>
        Defines the identity of a User. You must supply at least one of the available properties to identify the User.
        Any attributes (information held as name / value pairs) associated with the user may also be included.
      </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="ID" type="xs:int" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            Unique identity of a user.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Name" minOccurs="0" maxOccurs="1" type="LongReferenceType">
        <xs:annotation>
          <xs:documentation>
            Username.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="DeviceID" minOccurs="0" maxOccurs="1" type="LongReferenceType">
        <xs:annotation>
          <xs:documentation>
            ID of the device that the user is assigned to.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Attributes" type="AttributeCollectionType" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            Collection of Attributes (name / value pairs) associated with the user.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>


  <xs:element name="UserTypeCollection" type="UserTypeCollectionType" />
  <xs:complexType name="UserTypeCollectionType">
    <xs:annotation>
      <xs:documentation>
        Collection of users.
      </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="User" type="UserType" maxOccurs="unbounded" minOccurs="1">
      </xs:element>
    </xs:sequence>
  </xs:complexType>

  <xs:element name="DateTimeRange" type="DateTimeRangeType" />
  <xs:complexType name="DateTimeRangeType">
    <xs:annotation>
      <xs:documentation>
        Defines a DateTime Range.
      </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="StartDate" type="xs:dateTime" maxOccurs="1" minOccurs="1">
        <xs:annotation>
          <xs:documentation>
            The start of the DateTime range.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="EndDate" type="xs:dateTime" maxOccurs="1" minOccurs="1">
        <xs:annotation>
          <xs:documentation>
            The end of the DateTime range.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>

  <xs:element name="Message" type="MessageType" />
  <xs:complexType name="MessageType">
    <xs:annotation>
      <xs:documentation>
        Defines a Message.
      </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="Severity" type="SeverityType" minOccurs="1" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            Severity of the message.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Time" type="xs:dateTime" minOccurs="1" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            Time that the message was created.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Source" type="xs:string" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            Source of the message.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Text" type="xs:string" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            Text description of the message.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="More" type="xs:string" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            More information on the message.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>

  <xs:element name="Severity" type="SeverityType" />
  <xs:simpleType name="SeverityType">
    <xs:annotation>
      <xs:documentation>
        Defines the available levels of Severity
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="Information">
        <xs:annotation>
          <xs:documentation>
            Information
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Warning">
        <xs:annotation>
          <xs:documentation>
            Warning
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Error">
        <xs:annotation>
          <xs:documentation>
            Error
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>

  <xs:element name="OperationResult" type="OperationResultType" />
  <xs:simpleType name="OperationResultType">
    <xs:annotation>
      <xs:documentation>
        Indcates the current state of a Message based operation.
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="Pending">
        <xs:annotation>
          <xs:documentation>
            Pending
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Complete">
        <xs:annotation>
          <xs:documentation>
            Complete
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Deleted">
        <xs:annotation>
          <xs:documentation>
            Deleted
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Error">
        <xs:annotation>
          <xs:documentation>
            Error
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>

  <xs:element name="Contact" type="ContactType" />
  <xs:complexType name="ContactType">
    <xs:annotation>
      <xs:documentation>
        Defines a Contact.
      </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="Guid" type="GuidType" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            Internal unique identifier for the Contact.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Type" type="LongReferenceType" default="JM_PROPERTY" minOccurs="1" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            Classification for the Contact
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Reference" type="LongReferenceType" minOccurs="1" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            External unique identifier for the Contact
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ParentReference" type="LongReferenceType" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            External unique identifier for the parent Contact for this Contact
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Name" minOccurs="1" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            Name of the Contact
          </xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="255" />
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="Title" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            Title for the Contact
          </xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="100" />
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="Address" type="AddressType" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            Address of the Contact.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Phone" type="PhoneNumberType" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            Phone number of the Contact.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="WorkPhone" type="PhoneNumberType" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            Work Phone number of the Contact.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="MobilePhone" type="PhoneNumberType" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            Mobile Phone number of the Contact.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Email" type="ShortTextType" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            Email address of the Contact.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="URL" type="xs:anyURI" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            URL of the Contact.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Assets" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            Now obsolete, use AssetTasks if Assets are required for a Visit.
          </xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:sequence>
            <xs:element name="Asset" type="AssetType" maxOccurs="unbounded" minOccurs="0">
              <xs:annotation>
                <xs:documentation>
                  Description and identification details of an Asset.
                </xs:documentation>
              </xs:annotation>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="ExtendedProperties" type="ContactExtensionType" nillable="true" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            Extended information about the Contact.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>

  <xs:element name="ContactExtension" type="ContactExtensionType" />
  <xs:complexType name="ContactExtensionType">
    <xs:annotation>
      <xs:documentation>
        Defines a Contact Extension Type for holding additonal infomation about a contact.
      </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="PreferredName" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            A preferred name for the contact that could be different to their legal name.
          </xs:documentation>
        </xs:annotation>
        <xs:simpleType>
            <xs:restriction base="xs:string">
              <xs:maxLength value="35" />
            </xs:restriction>
          </xs:simpleType>
        </xs:element>
      <xs:element name="ID" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            An ID that is used to identify the contact in an external system.
          </xs:documentation>
        </xs:annotation>
      <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="20" />
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="IDLabel" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            A label used to identify the purpose of the ID value supplied. 
          </xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="20" />
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="DateOfBirth" type="xs:dateTime" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            The date of birth of the contact.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="DateOfDeath" type="xs:dateTime" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            The date of death of the contact.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Gender" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            The gender of the contact.
          </xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="20" />
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  
  <xs:element name="NameValuePair" type="NameValuePairType" />
  <xs:complexType name="NameValuePairType">
    <xs:annotation>
      <xs:documentation>
        Defines a type that consists of a Name and a Value.
      </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="Name" type="xs:string" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            Name of the pair.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Value" type="xs:string" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            Value of the pair.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>

  <xs:element name="NameValuePairCollection" type="NameValuePairCollectionType" />
  <xs:complexType name="NameValuePairCollectionType">
    <xs:annotation>
      <xs:documentation>
        Collection of name value pair items.
      </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="Item" type="NameValuePairType" maxOccurs="unbounded" minOccurs="0">
      </xs:element>
    </xs:sequence>
  </xs:complexType>

  <xs:element name="AttributeCollection" type="AttributeCollectionType" />
  <xs:complexType name="AttributeCollectionType">
    <xs:annotation>
      <xs:documentation>
        Collection of attributes (name / value pairs).
      </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="Attribute" type="NameValuePairType" minOccurs="0" maxOccurs="unbounded">
      </xs:element>
    </xs:sequence>
  </xs:complexType>

  <xs:element name="UserInfo" type="UserInfoType" />
  <xs:complexType name="UserInfoType">
    <xs:annotation>
      <xs:documentation>
        Defines a type used to represent the information about a user.
      </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="Identity" type="UserType" maxOccurs="1" minOccurs="1">
        <xs:annotation>
          <xs:documentation>
            Identity of a user.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Forename" type="LongReferenceType" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            Forename for the user.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Surname" type="LongReferenceType" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            Surname for the user.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="PhoneNo" type="LongReferenceType" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            Phone Number for the user.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Email" type="ShortTextType" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            Email address for the user.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="JobTitle" type="LongReferenceType" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            Job title for the user.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>

  <xs:element name="Guid" type="GuidType" />
  <xs:simpleType name="GuidType">
